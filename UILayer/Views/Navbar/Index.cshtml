@model List<HeaderListDto>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Inverse table</h4>
            <p class="card-description">
                Navbar
            </p>
            <div class="table-responsive">
                <table class="table table-gray">
                    <thead>
                        @foreach (var item in Model)
                        {
                        <tr>
                            <th> Resim </th>
                                <td> @item.Image </td>
                        </tr>
                        <tr>
                            <th> İsim </th>
                                <td> @item.Name </td>
                        </tr>
                        <tr>
                            <th> Yetenek 1 </th>
                                <td> @item.Title1 </td>
                        </tr>
                        <tr>
                            <th> Yetenek 2 </th>
                                <td> @item.Title2 </td>
                        </tr>
                        <tr>
                            <th> Yetenek 3 </th>
                                <td> @item.Title3 </td>
                        </tr>
                        <tr>
                            <th> Yetenek 4 </th>
                                <td> @item.Title4 </td>
                        </tr>
                        <tr>
                            <th> Twitter </th>
                                <td> @item.Twitter </td>
                        </tr>
                        <tr>
                            <th> Instagram </th>
                                <td> @item.Instagram </td>
                        </tr>
                        <tr>
                            <th> Github </th>
                                <td> @item.Github </td>
                        </tr>
                        <tr>
                            <th> Linkendin </th>
                                <td> @item.Linkendln </td>
                        </tr>
                        <td>
                            <a href="/Navbar/UpdateNavbar/@item.Id" class="btn btn-inverse-primary btn-fw">Güncelle</a>
                        </td>
                        }
                    </thead>
                    
                </table>
            </div>
        </div>
    </div>
</div>

