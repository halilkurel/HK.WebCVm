@model List<HeaderListDto>

<div class="col-lg-4 sticky-lg-top vh-100">
    @foreach (var item in Model)
    {
        <div class="d-flex flex-column h-100 text-center overflow-auto shadow">
            <img class="w-100 img-fluid mb-4" src="@item.Image" alt="Image">
            <h1 class="text-primary mt-2">@item.Name</h1>
            <div class="mb-4" style="height: 22px;">
                <h4 class="typed-text-output d-inline-block text-light"></h4>
                <div class="typed-text d-none">@item.Title1, @item.Title2, @item.Title3, @item.Title4</div>
            </div>
            <div class="d-flex justify-content-center mt-auto mb-3">
                <a class="btn btn-secondary btn-square mx-1" target="_blank" href="@item.Twitter"><i class="fab fa-twitter"></i></a>
                <a class="btn btn-secondary btn-square mx-1" target="_blank" href="@item.Github"><i class="fab fa-github"></i></a>
                <a class="btn btn-secondary btn-square mx-1" target="_blank" href="@item.Linkendln"><i class="fab fa-linkedin-in"></i></a>
                <a class="btn btn-secondary btn-square mx-1" target="_blank" href="@item.Instagram"><i class="fab fa-instagram"></i></a>
            </div>
        </div>
    }


</div>

