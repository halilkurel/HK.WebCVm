@model List<FooterListDto>

<!-- Footer Start -->
<section class="wow fadeIn" data-wow-delay="0.1s">

    <div class="bg-secondary text-light text-center p-5">
        @foreach (var item in Model)
        {
            <div class="d-flex justify-content-center mb-4">
                <a class="btn btn-dark btn-square mx-1" target="_blank" href="@item.Twitter"><i class="fab fa-twitter"></i></a>
                <a class="btn btn-dark btn-square mx-1" target="_blank" href="@item.LinkendIn"><i class="fab fa-linkedin-in"></i></a>
                <a class="btn btn-dark btn-square mx-1" target="_blank" href="@item.Instagram"><i class="fab fa-instagram"></i></a>
                <a class="btn btn-dark btn-square mx-1" target="_blank" href="@item.Github"><i class="fab fa-github"></i></a>
            </div>
            <div class="d-flex justify-content-center mb-3">
                <a class="text-light px-3" href="/Login/SignIn/">Admin</a>

            </div>

            <!--/*** This template is free as long as you keep the footer author’s credit link/attribution link/backlink. If you'd like to use the template without the footer author’s credit link/attribution link/backlink, you can purchase the Credit Removal License from "https://htmlcodex.com/credit-removal". Thank you for your support. ***/-->
            <p class="m-0">&copy; For All My Projects <a href="https://github.com/Halikkrl">Tüm Projeler</a></p>
        }
    </div>

</section>